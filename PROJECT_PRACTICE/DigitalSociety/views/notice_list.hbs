{{>index_header}}
<section class="content home">
            <div class="block-header">
                <div class="row">
                    <div class="col-lg-5 col-md-5 col-sm-12">
                        <h2>Blog List <small class="text-muted">Welcome to Oreo</small></h2>
                    </div>            
                    <div class="col-lg-7 col-md-7 col-sm-12 text-right">
                        
                        
                        
                        <ul class="breadcrumb float-md-right">
                            <li class="breadcrumb-item"><a href="..\dashboard\index.html"><i class="zmdi zmdi-home"></i> Oreo</a></li>
                                                            <li class="breadcrumb-item">Blog</li>
                                                                                        <li class="breadcrumb-item active">Blog List</li>
                                                    </ul>
                    </div>
                </div>
            </div>
            <div class="container-fluid">                
                <div class="blog-page">
    <div class="row">
        <div class="col-lg-12 col-md-12">
            
            {{#each noticeData}}
            <div class="card single_post">
                <div class="body">
                    <h3 class="m-t-0 m-b-5"><a href="detail.html">{{this.title}}</a></h3>
                </div>                    
                <div class="body">
                    <p>{{this.description}}</p>
                    <a href="edit?eid={{this._id}}" title="Edit" class="btn btn-round btn-edit">Edit</a>                        
                    <a href="del-notice?did={{this._id}}" title="Delete" class="btn btn-round btn-delete">Delete</a>

                    {{!-- <a href="delete?did={{this._id}}&&name={{this.name}}" title="Delete" class="btn btn-round btn-delete">Delete</a> --}}
                </div>
            </div>
            {{/each}}

                          
        </div>
       
    </div>
</div>
            </div>
</section>
{{>index_footer}}
